{
  "manifest_version": 3,
  "name": "Data Extractor",
  "version": "1.0.0",
  "description": "Extract data from search results and process them",
  "permissions": [
    "activeTab",
    "sidePanel",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://contactout.com/*",
    "https://www.fastpeoplesearch.com/*",
    "https://www.zabasearch.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Search Extractor"
  },
  "side_panel": {
    "default_path": "src/sidebar/sidebar.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Toggle sidebar"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*",
        "https://www.fastpeoplesearch.com/name*",
        "https://contactout.com/dashboard/search*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}